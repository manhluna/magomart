<% if(errors != "") { %>
    <% errors.forEach((item) => { %>
    <script> openLoginModal(); </script>
    <div class="alert alert-danger" id ="closeAlert"><%- item %></div>
    <% }); %>
<% } %>
<% if(success != "") { %>
    <% success.forEach((item) => { %>
    <script> openLoginModal(); </script>
    <div class="alert alert-success" id="closeAlert"><%- item %></div>
    <% }); %>
<% } %>